cp -r . ~/backups/meditation-app-$(date +%Y%m%d)/
ls -la ~/backups/meditation-app-$(date +%Y%m%d)/
#+end_src

*** Step 2: Create Migration Package

#+BEGIN_src bash
cd ~/LocalProjects/ai-bootcamp/meditation-app
mkdir -p migration-package/{presets,audio-cache}
cp backend/data/instructions.json migration-package/ 2>/dev/null || true
cp backend/data/api-keys.json migration-package/ 2>/dev/null || true
cp backend/data/presets/*.json migration-package/presets/ 2>/dev/null || true
cp -r backend/data/audio-cache/* migration-package/audio-cache/ 2>/dev/null || true
tar -czf meditation-app-migration-$(date +%Y%m%d).tar.gz migration-package/
ls -lh meditation-app-migration-*.tar.gz
#+end_src

*** Step 3: Transfer to Asamadhi

#+BEGIN_src bash
# From source directory to asamadhi's contemplation-machine
cd ~/LocalProjects/ai-bootcamp/meditation-app
scp meditation-app-migration-*.tar.gz asamadhi:~/LocalProjects/contemplation-machine/
#+end_src

*** Step 4: Extract & Migrate on Asamadhi

#+BEGIN_src bash
ssh asamadhi
cd ~/LocalProjects/contemplation-machine
Docker compose down
tar -xzf ~/meditation-app-migration-*.tar.gz -C .
#+end_src

*** Step 5: Restore Data Files

#+BEGIN_src bash
cd ~/LocalProjects/contemplation-machine
# Backup current data
cp -r backend/data backend/data.backup

# Restore custom data
cp migration-package/instructions.json backend/data/ 2>/dev/null || true
cp migration-package/api-keys.json backend/data/ 2>/dev/null || true
cp migration-package/presets/*.json backend/data/presets/ 2>/dev/null || true
cp -r migration-package/audio-cache/* backend/data/audio-cache/ 2>/dev/null || true

ls -la backend/data/
#+end_src

*** Step 6: Restart & Verify

#+BEGIN_src bash
cd ~/LocalProjects/contemplation-machine
docker compose up -d
docker compose logs backend | tail -10
curl http://localhost:3001/api/instructions | head -50
curl http://localhost:3001/api/presets | head -50
#+end_src

*Result:* Your custom instructions, presets, and audio files are now migrated.

*** Step 7: Cleanup

#+BEGIN_src bash
cd ~/LocalProjects/contemplation-machine
rm -rf migration-package
rm -f meditation-app-migration-*.tar.gz
echo \"Migration complete and cleaned up\"
